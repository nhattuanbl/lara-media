{
    "name": "nhattuanbl/lara-media",
    "description": "Laravel media manager / convert",
    "keywords": ["laravel", "storage", "flysystem", "filesystem"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nhattuanbl\\LaraMedia\\": "src/"
        }
    },
    "authors": [
        {
            "name": "nhattuanbl",
            "email": "nhattuanbl@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Nhattuanbl\\LaraMedia\\LaraMediaProvider"
            ],
            "aliases": {

            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../lara-helper"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-mongodb": "*",
        "ext-imagick": "*",
        "laravel/framework": "*",
        "ralouphie/mimey": "*",
        "nhattuanbl/lara-helper": "*",
        "guzzlehttp/guzzle": "*",
        "php-ffmpeg/php-ffmpeg": "*",
        "livewire/livewire": ">=3",
        "intervention/mimesniffer": "*"
    }
}
